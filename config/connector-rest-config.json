{  
   "name":"@CONNECTOR_NAME@",
   "config":{  
      "connector.class":"com.couchbase.connect.kafka.CouchbaseSourceConnector",
      "connection.cluster_address":"@COUCHBASE_NODES@",
      "connection.bucket":"@COUCHBASE_BUCKET@",
      "connection.timeout.ms":"2000",
      "tasks.max":"3",
      "topic.name":"@KAFKA_TOPIC@",
      "use_snapshots":"false",
      "dcp.message.converter.class":"@CONVERTER_CLASS@",
      "event.filter.class":"@FILTER_CLASS@",
      "couchbase.stream_from":"@COUCHBASE_FROM_TO@"
   }
}